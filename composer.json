{
    "name": "matthimatiker/ontowiki",
    "description": "Semantic data wiki as well as Linked Data publishing engine",
    "keywords": ["semantic", "wiki", "rdf", "sparql", "endpoint"],
    "homepage": "http://ontowiki.net/",
    "minimum-stability": "stable",
    "include-path": [
        "library"
    ],
    "autoload": {
        "psr-0": {
            "Erfurt_": "library"
        }
    },
    "require": {
        "zendframework/zendframework1": ">=1.11.5",
        "matthimatiker/erfurt": "dev-develop",
        "aksw/rdfauthor": "*",
        "everyman/neo4jphp": "dev-master"
    },
    "suggest": {
    },
    "config": {
        "use-include-path": true
    },
    "extra": {
        "installation-paths": {
            "aksw/rdfauthor": "libraries/RDFauthor"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Matthimatiker/Erfurt"
        },
        {
            "type": "package",
            "package": {
                "name": "aksw/rdfauthor",
                "type": "marshaller-asset",
                "version": "0.9.6",
                "dist": {
                    "url": "https://github.com/AKSW/RDFauthor/archive/v0.9.6.zip",
                    "type": "zip"
                },
                "require": {
                    "matthimatiker/marshaller": "*"
                }
            }
        }
    ]
}