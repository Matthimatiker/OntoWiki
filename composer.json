{
    "name": "matthimatiker/ontowiki",
    "description": "Semantic data wiki as well as Linked Data publishing engine",
    "keywords": ["semantic", "wiki", "rdf", "sparql", "endpoint"],
    "homepage": "http://ontowiki.net/",
    "minimum-stability": "stable",
    "include-path": [
        "library"
    ],
    "autoload": {
        "psr-0": {
            "Erfurt_": "library"
        }
    },
    "require": {
        "aksw/erfurt": "dev-feature/sync_with_aksw",
        "zendframework/zendframework1": ">=1.11.5",
        "aksw/rdfauthor": "*",
        "everyman/neo4jphp": "dev-master"
    },
    "suggest": {
    },
    "config": {
        "use-include-path": true
    },
    "extra": {
        "installation-paths": {
            "aksw/rdfauthor": "libraries/RDFauthor"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "aksw/erfurt",
                "version": "dev-feature/sync_with_aksw",
                "dist": {
                    "url": "https://github.com/Matthimatiker/Erfurt/archive/feature/sync_with_aksw.zip",
                    "type": "zip"
                },
                "include-path": [
                    "library"
                ],
                "autoload": {
                    "psr-0": {
                        "Erfurt_": "library"
                    }
                },
                "require": {
                    "zendframework/zendframework1": ">=1.11.5",
                    "doctrine/dbal": "~2.4.1",
                    "symfony/config": "~2.3.0",
                    "symfony/dependency-injection": "~2.3.0",
                    "soundintheory/php-sql-parser": "~1.0.0",
                    "guzzle/guzzle": "~3.7.0"
                },
                "require-dev": {
                    "athletic/athletic": "dev-master",
                    "fzaninotto/faker": "~1.3.0"
                },
                "suggest": {
                    "ext-oci8": "To use the Oracle Triple Store."
                },
                "config": {
                    "use-include-path": true
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "aksw/rdfauthor",
                "type": "marshaller-asset",
                "version": "0.9.6",
                "dist": {
                    "url": "https://github.com/AKSW/RDFauthor/archive/v0.9.6.zip",
                    "type": "zip"
                },
                "require": {
                    "matthimatiker/marshaller": "*"
                }
            }
        }
    ]
}